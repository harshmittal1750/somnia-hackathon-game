{
    "version": 2,
    "builds": [
        {
            "src": "server-minimal.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server-minimal.js"
        },
        {
            "src": "/health",
            "dest": "/server-minimal.js"
        },
        {
            "src": "/test",
            "dest": "/server-minimal.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server-minimal.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}